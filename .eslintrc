{
  "ecmaFeatures": {
    "arrowFunctions": true,
    "blockBindings": true,
    "classes": true,
    "defaultParams": true,
    "destructuring": true,
    "forOf": true,
    "modules": true,
    "objectLiteralComputedProperties": true,
    "objectLiteralShorthandMethods": true,
    "objectLiteralShorthandProperties": true,
    "restParams": true,
    "spread": true,
    "templateStrings": true
  },
  "parser": "babel-eslint",
  "rules": {
    "accessor-pairs": 2,
    "block-scoped-var": 2,
    "curly": 2,
    "default-case": 1,
    "dot-notation": 1,
    "dot-location": [2, "property"],
    "eqeqeq": 2,
    "guard-for-in": 1,
    "no-caller": 2,
    "no-else-return": 1,
    "no-empty-label": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-fallthrough": 2,
    "no-floating-decimal": 2,
    "no-implied-eval": 2,
    "no-iterator": 2,
    "no-lone-blocks": 2,
    "no-loop-func": 2,
    "no-native-reassign": 2,
    "no-new-func": 2,
    "no-new-wrappers": 2,
    "no-new": 2,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-return-assign": 1,
    "no-sequences": 2,
    "no-throw-literal": 2,
    "no-undef": 2,
    "no-unused-expressions": 2,
    "no-unused-vars": [2, { "argsIgnorePattern": "^_.*" }],
    "no-useless-call": 2,
    "no-useless-concat": 1 ,
    "no-with": 2,
    "yoda": 2
  }
}
